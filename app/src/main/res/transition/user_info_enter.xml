<?xml version="1.0" encoding="utf-8"?>
<transitionSet xmlns:android="http://schemas.android.com/apk/res/android"
    android:duration="@integer/shortHalf">
    <fade>
        <targets>
            <target android:excludeId="@+id/rl_panel" />
        </targets>
    </fade>
    <transition
        class="com.ashwashing.pro.ui.transition.ClipSlideDown"
        android:interpolator="@android:interpolator/fast_out_slow_in">
        <targets>
            <target android:targetId="@+id/rl_panel" />
        </targets>
    </transition>
</transitionSet>